        <name>create_event</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    // implement resource here

		var requestBody = request.body.data;
		
	var _eventGR = new GlideRecord('x_1539926_event_ma_event');
	_eventGR.initialize();
	
	_eventGR.x_event_name = requestBody.x_event_name;
	_eventGR.x_location = requestBody.x_location;
	_eventGR.x_conatct_person = requestBody.x_contact_person;


	var sysId = _eventGR.insert();

	if(!sysId) {
		response.setStatus(500);
		return { error: 'Failed to create patient record' };
	}

	response.setStatus(201);
    return {
        message: 'Event record created successfully',
        sys_id: sysId
    };

})(request, response);]]></operation_script>
