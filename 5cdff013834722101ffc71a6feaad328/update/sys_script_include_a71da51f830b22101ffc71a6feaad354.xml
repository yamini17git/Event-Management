        <script><![CDATA[var fetch_event_details_client = Class.create();
fetch_event_details_client.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	fetchEventDetails:function(){
	var eventId = this.getParameter('sysparm_event_id');
	var grEvent = new GlideRecord('x_1790002_event_ma_event_record');
	if(grEvent.get(eventId))
	{
		return JSON.stringify({
			name: grEvent.getDisplayValue('event_name'),
			date: grEvent.getDisplayValue('event_date'),
			status: grEvent.getDisplayValue('status')
		});
	}
	return '';
	},

    type: 'fetch_event_details_client'
}

);]]></script>
