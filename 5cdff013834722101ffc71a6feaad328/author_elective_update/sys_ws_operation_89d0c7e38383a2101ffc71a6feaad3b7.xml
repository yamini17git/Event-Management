        <http_method>POST</http_method>
        <name>Create Event</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    // implement resource here
    var body=request.body.data;
    var gr=new GlideRecord('x_1539926_event_ma_event');
    gr.initialize();
    gr.x_event_name = body.event_name;
    // gr.x_event_date = body.event_date;
    gr.x_event_location = body.location;
    gr.x_conatct_person = body.contact_person;

    var success=gr.insert();

    if(success)
    {
        response.setStatus(201);
        response.setBody({status:'success',event_sys_id:gr.getUniqueValue()});
    }
    else
    {
        response.setStatus(500);
        response.setBody({ status: 'failure', message: 'Event creation failed' });
    }



})(request, response);]]></operation_script>
